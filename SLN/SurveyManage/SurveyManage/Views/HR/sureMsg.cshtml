@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <link href="~/img/logo.ico" rel='icon' type=‘image/x-ico’ />
    <script src="~/EasyUI/jquery.min.js"></script>
    <script src="~/EasyUI/jquery.easyui.min.js"></script>
     <link href="~/EasyUI/themes/icon.css" rel="stylesheet" />
    <link href="~/EasyUI/themes/gray/easyui.css" rel="stylesheet" />
    <script src="~/JS/sureMsg.js"></script>
    <link href="~/CSS/sureMsg.css" rel="stylesheet" />
   <script>
       $("#win").ready(function ()
       {
           $("#win").dialog({
               width: 490,
               height: 300,
               title: "基本信息确认",
               buttons: [{
                   iconCls: "icon-ok",
                   text: "确定",
                   handler: function ()
                   {
                       window.location.href = "/HR/Index";
                   }
               }]
           });
       });

   </script>
   <style>
   html {
            background: url(data:image/gif;base64,) repeat top left fixed;
        }
   </style>
    <title>信息确认</title>
</head>
<body>
    
    <div id="win" >   
        <ul>
            <li>姓名：<span class="liText">@ViewBag.name</span></li>
            <li>公司：<span class="liText">@ViewBag.companyName</span></li>
            <li>电话：<span class="liText">@ViewBag.phone</span></li>
            <li>邮箱：<span class="liText">@ViewBag.email</span></li>
             
        </ul>  
</div>  
</body>
</html>
